<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/kbc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/kbc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".chemistry_quiz"></activity>
        <activity android:name=".physics_quiz" />
        <activity android:name=".geography_quiz" />
        <activity android:name=".politics_quiz" />
        <activity android:name=".bollywood_quiz" />
        <activity android:name=".sports_quiz" />
        <activity android:name=".Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>